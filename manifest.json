{
  "manifest_version": 2,
  "name": "GuitarTabFinder",
  "version": "1.0",
  "permissions": ["tabs", "activeTab", "https://*/"],
  "browser_action": {
    "default_icon": {
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_popup": "index.html",
    "default_title": "Guitar Tab Finder"
  }
}
